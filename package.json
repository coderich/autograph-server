{
  "name": "@coderich/autograph",
  "author": "Richard Livolsi (coderich)",
  "version": "0.0.5",
  "description": "",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "start": "node ./src/server",
    "test": "node ./node_modules/.bin/jest test --passWithNoTests --detectOpenHandles --forceExit --runInBand",
    "test-inspect": "node --inspect-brk --inspect ./node_modules/.bin/jest test --passWithNoTests --detectOpenHandles --forceExit --runInBand",
    "inspect": "node --expose-gc --inspect=9222 ./src/server",
    "uuid": "uuid"
  },
  "dependencies": {
    "@hapi/boom": "^8.0.1",
    "apollo-link-ws": "^1.0.19",
    "axios": "^0.19.0",
    "change-case": "^4.1.0",
    "dataloader": "^2.0.0",
    "deepmerge": "^4.2.2",
    "graphql-subscriptions": "^1.1.0",
    "lodash": "^4.17.15",
    "mongodb": "^3.3.5",
    "neo4j-driver": "^4.0.0",
    "object-hash": "^2.0.1",
    "picomatch": "^2.1.1",
    "redis": "^2.8.0",
    "uuid": "^3.3.3",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@hapi/hapi": "^18.4.0",
    "apollo-server-hapi": "^2.9.13",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "graphql": "^14.5.8",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.1",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "mongodb-memory-server": "^6.0.2",
    "neodb": "^3.0.0",
    "redis-mock": "^0.47.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/*/**/*.js"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "suiteName": "autograph",
          "output": "<rootDir>/coverage/test-reports/junit/junit.xml"
        }
      ]
    ],
    "setupFilesAfterEnv": [],
    "testPathIgnorePatterns": [
      "<rootDir>/test/integration"
    ]
  }
}
