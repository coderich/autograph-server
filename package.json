{
  "name": "@coderich/autograph",
  "author": "Richard Livolsi (coderich)",
  "version": "0.0.6",
  "description": "The fastest way to GraphQL",
  "main": "index.js",
  "license": "ISC",
  "files": [
    "src/",
    "errors.js",
    "index.js",
    "rules.js",
    "schema.js",
    "transforms.js",
    "types.js"
  ],
  "scripts": {
    "start": "node ./src/server",
    "test": "jest test --passWithNoTests --detectOpenHandles --forceExit --runInBand",
    "test-inspect": "node --inspect-brk --inspect ./node_modules/.bin/jest test --passWithNoTests --detectOpenHandles --forceExit --runInBand",
    "inspect": "node --expose-gc --inspect=9222 ./src/server",
    "lint": "ratchet lint",
    "ratchet": "ratchet",
    "uuid": "uuid"
  },
  "dependencies": {
    "apollo-link-ws": "^1.0.19",
    "axios": "^0.19.0",
    "change-case": "^4.1.0",
    "dataloader": "^2.0.0",
    "deepmerge": "^4.2.2",
    "fill-range": "^7.0.1",
    "graphql-fields": "^2.0.3",
    "graphql-subscriptions": "^1.1.0",
    "lodash": "^4.17.15",
    "micromatch": "^4.0.2",
    "mongodb": "^3.3.5",
    "neo4j-driver": "^4.0.0",
    "object-hash": "^2.0.1",
    "picomatch": "^2.1.1",
    "redis": "^2.8.0",
    "uuid": "^3.3.3",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@coderich/ratchet": "^1.2.5",
    "apollo-server": "^2.9.15",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "graphql": "^14.5.8",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.1",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "mongodb-memory-server": "^6.0.2",
    "neodb": "^3.0.0",
    "redis-mock": "^0.47.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/*/**/*.js"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "suiteName": "autograph",
          "output": "<rootDir>/coverage/test-reports/junit/junit.xml"
        }
      ]
    ],
    "setupFilesAfterEnv": [],
    "testPathIgnorePatterns": [
      "<rootDir>/test/integration"
    ]
  }
}
